const <%= config.database.name %> = require('../../persistence/<%= config.database.file %>')
<% if (config.database.isMongoDB) { -%>
const <%= config.title %> = require('./<%= config.name %>.model')

module.exports = new <%= config.database.name %>(<%= config.title %>)
<% } else { %>
module.exports = new <%= config.database.name %>('<%= config.name %>s')
<% } -%>
